l1 = [1, 8, 2, 3, 4, 6]
output = []

for i in range(len(l1)):
    if i + l1[i] > len(l1):  
        output.append([0])  # If index goes out of bounds, append [0]
    else:
        output.append(l1[i:i + l1[i]])

# Print the output in the desired format
for item in output:
    print(item)
